        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.theamazingjosh.insta"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3f2fb991-a8eb-4f02-9dfc-e1b1ce4e22a5"
      }
    }
  }
